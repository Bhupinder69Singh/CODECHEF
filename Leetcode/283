//move zeroes
class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int i=0,j=0;
        int k=nums.size();
        while(j<k){
            if(nums[j] !=0){
                swap(nums[i],nums[j]);
                i++;
                j++;
            }
            else{
                j++;
            }
        }
    }
};
